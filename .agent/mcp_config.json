{
    "mcpServers": {
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp",
                "--api-key",
                " "
            ]
        },
        "shadcn": {
            "command": "npx",
            "args": [
                "shadcn@latest",
                "mcp"
            ]
        },
        "github": {
            "serverUrl": "https://api.githubcopilot.com/mcp/",
            "headers": {
                "Authorization": "Bearer ENV_GITHUB_PAT"
            }
        }
    }
}